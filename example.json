{
	"port": 3000,
	"tls_files": [
		{
			"cert_file": "/etc/letsencrypt/live/example.com/cert.pem",
			"key_file": "/etc/letsencrypt/live/example.com/privkey.pem"
		},
		{
			"cert_file": "/etc/letsencrypt/live/myownpersonaldomain.com/cert.pem",
			"key_file": "/etc/letsencrypt/live/myownpersonaldomain.com/privkey.pem"
		}
	],
	"mapping": [
		{
			"host": "example.com",
			"use_tls": false,
			"port": 1234
		},
		{
			"host": "example.com",
			"use_tls": true,
			"port": 1235
		},
		{
			"host": "myownpersonaldomain.com",
			"use_tls": false,
			"port": 1236
		},
		{
			"host": "myownpersonaldomain.com",
			"use_tls": true,
			"port": 1237
		}
	]
}
